{
  "name": "@endo/daemon",
  "version": "2.3.0",
  "private": true,
  "description": "Endo daemon",
  "keywords": [
    "endo",
    "daemon"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/blob/master/packages/daemon/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/daemon"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./types.d.ts",
  "exports": {
    ".": {
      "types": "./types.d.ts",
      "default": "./index.js"
    },
    "./ref-reader.js": "./ref-reader.js",
    "./reader-ref.js": "./reader-ref.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "build:types": "tsc --build tsconfig.build.json",
    "clean:types": "git clean -f '*.d.ts*'",
    "cover": "c8 ava",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint-fix": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:types": "tsc",
    "test": "ava",
    "test:clean": "rm -rf tmp && yarn test"
  },
  "dependencies": {
    "@endo/base64": "^1.0.5",
    "@endo/captp": "^4.2.0",
    "@endo/compartment-mapper": "^1.1.5",
    "@endo/errors": "^1.2.2",
    "@endo/eventual-send": "^1.2.2",
    "@endo/exo": "^1.5.0",
    "@endo/far": "^1.1.2",
    "@endo/import-bundle": "^1.1.2",
    "@endo/init": "^1.1.2",
    "@endo/lockdown": "^1.0.7",
    "@endo/marshal": "^1.5.0",
    "@endo/netstring": "^1.0.7",
    "@endo/patterns": "^1.4.0",
    "@endo/promise-kit": "^1.1.2",
    "@endo/stream": "^1.2.2",
    "@endo/stream-node": "^1.1.2",
    "@endo/where": "^1.0.5",
    "ses": "^1.5.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@endo/bundle-source": "^3.2.3",
    "@endo/ses-ava": "^1.2.2",
    "ava": "^6.1.2",
    "babel-eslint": "^10.0.3",
    "c8": "^7.14.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.2.5",
    "typescript": "~5.5.0-dev.20240327"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/daemon"
    ]
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  },
  "typeCoverage": {
    "atLeast": 97.21
  }
}
