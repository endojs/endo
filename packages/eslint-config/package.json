{
  "name": "@endo/eslint-config",
  "version": "0.4.2",
  "description": "Endo style rules",
  "keywords": [
    "endo",
    "eslint-config"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "scripts": {
    "build": "exit 0",
    "lint": "prettier --check *.js, *.yaml",
    "lint-fix": "prettier --write *.js, *.yaml",
    "test": "node ./index.js"
  },
  "dependencies": {
    "@endo/eslint-plugin": "^0.3.20",
    "@jessie.js/eslint-plugin": "^0.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.10.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-jessie": "^0.0.4",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsdoc": "^30.4.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "prettier": "^1.19.1",
    "typescript": "~4.5.5"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "index.js",
    "default.yaml"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
