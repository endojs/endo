{
  "name": "@endo/non-trapping-shim",
  "version": "0.1.0",
  "private": true,
  "description": "shim of the non-trapping integrity trait",
  "keywords": [
    "stabilize",
    "non-trapping",
    "Proxy",
    "shim"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/non-trapping-shim#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/non-trapping-shim"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./shim.js",
  "module": "./shim.js",
  "exports": {
    "./shim.js": "./shim.js",
    "./prepare-enable-shim.js": "./prepare-enable-shim.js",
    "./non-trapping-shim-adapter.js": "./non-trapping-shim-adapter.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint-fix": "yarn lint:eslint --fix && yarn lint:types",
    "lint:eslint": "eslint '**/*.js'",
    "lint:types": "tsc",
    "postpack": "git clean -fX \"*.d.ts*\" \"*.d.cts*\" \"*.d.mts*\" \"*.tsbuildinfo\"",
    "prepack": "tsc --build tsconfig.build.json",
    "test": "ava",
    "test:c8": "c8 ${C8_OPTIONS:-} ava",
    "test:xs": "exit 0"
  },
  "dependencies": {
    "@endo/env-options": "workspace:^",
    "@endo/harden": "workspace:^"
  },
  "devDependencies": {
    "ava": "catalog:dev",
    "c8": "catalog:dev",
    "eslint": "catalog:dev",
    "tsd": "catalog:dev",
    "typescript": "~5.9.2"
  },
  "files": [
    "./*.d.ts",
    "./*.js",
    "./*.map",
    "LICENSE*",
    "SECURITY*",
    "dist",
    "lib",
    "src",
    "tools"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.*"
    ],
    "timeout": "2m"
  }
}
