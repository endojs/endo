// This file can contain .js-specific Typescript compiler config.
{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "checkJs": true,
    "noEmit": true,
    "downlevelIteration": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "types": []
  },
  "include": [
    "*.js",
    "*.ts",
    "src/**/*.js",
    "src/**/*.ts"
  ]
}
