    "@endo/far": "^0.2.18",
    "@endo/lockdown": "^0.1.28",
    "@endo/promise-kit": "^0.2.56",
    "@endo/where": "^0.3.1",
    "commander": "^5.0.0",
    "ses": "^0.18.4"
  },
  "devDependencies": {
    "ava": "^5.3.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.8",
    "typescript": "~4.9.5"
