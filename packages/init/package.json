{
  "name": "@endo/init",
  "version": "0.6.0",
  "description": "Prepare Endo environment on import",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./debug.js": "./debug.js",
    "./legacy.js": "./legacy.js",
    "./unsafe-fast.js": "./unsafe-fast.js",
    "./pre.js": {
      "node": "./src/pre-node.js",
      "default": "./pre.js"
    },
    "./pre-remoting.js": "./pre-remoting.js",
    "./pre-bundle-source.js": "./pre-bundle-source.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "prepack": "tsc --build jsconfig.build.json",
    "postpack": "git clean -f '*.d.ts*'",
    "test": "ava",
    "test:xs": "exit 0",
    "lint-check": "yarn lint",
    "lint-fix": "eslint --fix '**/*.js'",
    "lint": "yarn lint:types && eslint '**/*.js'",
    "lint:types": "tsc -p jsconfig.json"
  },
  "devDependencies": {
    "@endo/compartment-mapper": "^0.9.0",
    "@endo/ses-ava": "^0.2.41",
    "ava": "^5.1.1"
  },
  "dependencies": {
    "@endo/base64": "^0.2.32",
    "@endo/eventual-send": "^0.18.0",
    "@endo/lockdown": "^0.1.29",
    "@endo/promise-kit": "^0.2.57"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git"
  },
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "homepage": "https://github.com/endojs/endo#readme",
  "ava": {
    "files": [
      "test/**/test-*.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "eslintIgnore": [
    "bundle-*.js"
  ],
  "publishConfig": {
    "access": "public"
  }
}
