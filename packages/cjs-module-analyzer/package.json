{
  "name": "@endo/cjs-module-analyzer",
  "version": "1.0.2",
  "description": "A JavaScript lexer dedicated to static analysis and transformation of ECMAScript modules.",
  "keywords": [],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/cjs-module-analyzer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/cjs-module-analyzer"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": "./index.js"
  },
  "scripts": {
    "build": "exit 0",
    "build:types": "tsc --build tsconfig.build.json",
    "clean:types": "git clean -f '*.d.ts*'",
    "cover": "c8 ava",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint-fix": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:types": "tsc",
    "test": "ava"
  },
  "devDependencies": {
    "ava": "^6.1.2",
    "babel-eslint": "^10.0.3",
    "c8": "^7.14.0",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.0.0",
    "typescript": "~5.4.2"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "*.js",
    "*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  },
  "typeCoverage": {
    "atLeast": 39.57
  }
}
