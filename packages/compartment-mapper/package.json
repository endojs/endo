{
  "name": "@endo/compartment-mapper",
  "version": "1.1.5",
  "description": "The compartment mapper assembles Node applications in a sandbox",
  "keywords": [
    "node",
    "endo",
    "package",
    "compartment"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/compartment-mapper#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/compartment-mapper"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "types": "./types.d.ts",
  "exports": {
    ".": {
      "types": "./types.d.ts",
      "default": "./index.js"
    },
    "./import.js": "./import.js",
    "./archive.js": "./archive.js",
    "./import-archive.js": "./import-archive.js",
    "./bundle.js": "./bundle.js",
    "./node-powers.js": "./node-powers.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -f '*.d.ts*'",
    "cover": "c8 ava",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint-fix": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:types": "tsc",
    "prettier-fixtures": "prettier --write --with-node-modules './test/fixtures-*/**/*.*js'",
    "test": "ava"
  },
  "dependencies": {
    "@endo/cjs-module-analyzer": "workspace:*",
    "@endo/static-module-record": "workspace:*",
    "@endo/zip": "workspace:*",
    "ses": "^1.5.0"
  },
  "devDependencies": {
    "ava": "^6.1.2",
    "babel-eslint": "^10.0.3",
    "c8": "^7.14.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.2.5",
    "typescript": "5.5.0-beta"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.cjs",
        "options": {
          "parser": "babel"
        }
      }
    ]
  },
  "ava": {
    "files": [
      "test/**/test-*.*",
      "test/**/*.test.*"
    ],
    "timeout": "2m"
  },
  "typeCoverage": {
    "atLeast": 86.14
  }
}
