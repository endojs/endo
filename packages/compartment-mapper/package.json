{
  "name": "@endo/compartment-mapper",
  "version": "0.9.1",
  "description": "The compartment mapper assembles Node applications in a sandbox",
  "keywords": [
    "node",
    "endo",
    "package",
    "compartment"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/compartment-mapper#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "types": "./types.d.ts",
  "exports": {
    ".": "./index.js",
    "./import.js": "./import.js",
    "./archive.js": "./archive.js",
    "./import-archive.js": "./import-archive.js",
    "./bundle.js": "./bundle.js",
    "./node-powers.js": "./node-powers.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "prepack": "tsc --build jsconfig.build.json",
    "postpack": "git clean -f '*.d.ts*'",
    "cover": "c8 ava",
    "lint": "pnpm lint:types && pnpm lint:js",
    "lint-fix": "eslint --fix .",
    "lint:js": "eslint .",
    "lint:types": "tsc -p jsconfig.json",
    "prettier-fixtures": "prettier --write --with-node-modules './test/fixtures-*/**/*.*js'",
    "test": "ava"
  },
  "dependencies": {
    "@endo/cjs-module-analyzer": "workspace:*",
    "@endo/eventual-send": "workspace:*",
    "@endo/static-module-record": "workspace:*",
    "@endo/zip": "workspace:*",
    "ses": "^0.18.7"
  },
  "devDependencies": {
    "@types/node": "^16.6.0",
    "ava": "^5.3.0",
    "babel-eslint": "^10.0.3",
    "c8": "^7.14.0",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^3.0.0",
    "typescript": "~5.1.3"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.cjs",
        "options": {
          "parser": "babel"
        }
      }
    ]
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  }
}
