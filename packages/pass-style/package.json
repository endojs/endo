    "babel-eslint": "^10.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.5",
    "typescript": "~4.9.5"
