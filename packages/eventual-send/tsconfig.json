{
  "extends": "../../tsconfig.eslint-base.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "skipLibCheck": false
  },
  "include": [
    "*.js",
    "*.ts",
    "src/**/*.js",
    "src/**/*.ts"
  ]
}
