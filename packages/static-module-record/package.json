{
  "name": "@endo/static-module-record",
  "version": "0.6.3",
  "description": "Shim for the SES StaticModuleRecord and module-to-program transformer",
  "keywords": [
    "ses",
    "StaticModuleRecord",
    "modules",
    "ESM"
  ],
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/static-module-record#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "types": "./index.d.ts",
  "main": "./index.js",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "cover": "c8 ava",
    "lint": "yarn lint:types && yarn lint:js",
    "lint:types": "tsc --build jsconfig.json",
    "lint:js": "eslint '**/*.js'",
    "lint-fix": "eslint --fix '**/*.js'",
    "test": "ava"
  },
  "dependencies": {
    "@agoric/babel-standalone": "^7.14.3"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@endo/eslint-config": "^0.3.15",
    "@endo/ses-ava": "^0.2.8",
    "ava": "^3.12.1",
    "babel-eslint": "^10.0.3",
    "c8": "^7.7.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "ses": "^0.14.3",
    "typescript": "^4.0.5"
  },
  "files": [
    "LICENSE*",
    "src",
    "index.js",
    "index.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  }
}
