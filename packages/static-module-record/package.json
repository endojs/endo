{
  "name": "@endo/static-module-record",
  "version": "0.7.16",
  "description": "Shim for the SES StaticModuleRecord and module-to-program transformer",
  "keywords": [
    "ses",
    "StaticModuleRecord",
    "modules",
    "ESM"
  ],
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/static-module-record#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "types": "./index.d.ts",
  "main": "./index.js",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "cover": "c8 ava",
    "lint": "yarn lint:types && yarn lint:js",
    "lint:types": "tsc -p jsconfig.json",
    "lint:js": "eslint .",
    "lint-fix": "eslint --fix .",
    "test": "ava"
  },
  "dependencies": {
    "@agoric/babel-generator": "^7.17.6",
    "@babel/parser": "^7.17.3",
    "@babel/traverse": "^7.17.3",
    "@babel/types": "^7.17.0",
    "ses": "^0.18.1"
  },
  "devDependencies": {
    "@endo/eslint-config": "^0.5.2",
    "@endo/ses-ava": "^0.2.37",
    "ava": "^5.1.1",
    "babel-eslint": "^10.0.3",
    "benchmark": "^2.1.4",
    "c8": "^7.7.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.8.0",
    "typescript": "~4.9.4"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "trailingComma": "all",
    "singleQuote": true
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  }
}
