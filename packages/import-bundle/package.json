{
  "name": "@endo/import-bundle",
  "version": "0.3.0",
  "description": "load modules created by @endo/bundle-source",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "build": "exit 0",
    "lint-check": "yarn lint",
    "lint-fix": "eslint --fix '**/*.js'",
    "lint": "eslint '**/*.js'"
  },
  "dependencies": {
    "@endo/base64": "^0.2.28",
    "@endo/compartment-mapper": "^0.8.0"
  },
  "devDependencies": {
    "@endo/bundle-source": "^2.4.2",
    "@endo/init": "^0.5.52",
    "@endo/ses-ava": "^0.2.36",
    "ava": "^5.1.0",
    "c8": "^7.7.3"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "homepage": "https://github.com/endojs/endo/tree/master/packages/import-bundle",
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  }
}
