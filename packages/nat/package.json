{
  "name": "@endo/nat",
  "version": "5.1.3",
  "description": "Ensures that a number is within the natural numbers (0, 1, 2...) or throws a RangeError",
  "main": "./src/index.js",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -fX \"*.d.ts*\" \"*.d.cts*\" \"*.d.mts*\" \"*.tsbuildinfo\"",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint-fix": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:types": "tsc",
    "test": "ses-ava",
    "test:c8": "c8 ${C8_OPTIONS:-} ses-ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/nat"
  },
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "homepage": "https://github.com/endojs/endo#readme",
  "devDependencies": {
    "@endo/compartment-mapper": "workspace:^",
    "@endo/ses-ava": "workspace:^",
    "ava": "catalog:dev",
    "c8": "catalog:dev",
    "eslint": "catalog:dev",
    "ses": "workspace:^",
    "typescript": "~5.9.2"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "integer",
    "int",
    "overflow"
  ],
  "files": [
    "./*.d.ts",
    "./*.js",
    "./*.map",
    "LICENSE*",
    "SECURITY*",
    "dist",
    "lib",
    "src",
    "tools"
  ],
  "sesAvaConfigs": {
    "shims": "../../ava-endo-shims-only.config.mjs",
    "lockdown": "../../ava-endo-lockdown.config.mjs",
    "unsafe": "../../ava-endo-lockdown-unsafe.config.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  }
}
