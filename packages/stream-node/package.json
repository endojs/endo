{
  "name": "@endo/stream-node",
  "version": "1.1.2",
  "description": "Uint8Array async iterator adapters for Node.js streams",
  "keywords": [
    "stream",
    "node",
    "async",
    "iterator",
    "promise"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/stream-node#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/stream-node"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "exports": {
    ".": "./index.js",
    "./reader.js": "./reader.js",
    "./writer.js": "./writer.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -f '*.d.ts*'",
    "cover": "c8 ava",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint-fix": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:types": "tsc",
    "test": "ava"
  },
  "dependencies": {
    "@endo/errors": "workspace:*",
    "@endo/init": "workspace:*",
    "@endo/stream": "workspace:*",
    "ses": "^1.5.0"
  },
  "devDependencies": {
    "@endo/ses-ava": "workspace:*",
    "ava": "^6.1.2",
    "babel-eslint": "^10.0.3",
    "c8": "^7.14.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.2.5",
    "ts-api-utils": "~1.0.1",
    "typescript": "5.5.0-beta"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "ava": {
    "files": [
      "test/**/test-*.*",
      "test/**/*.test.*"
    ],
    "timeout": "2m"
  },
  "typeCoverage": {
    "atLeast": 98.69
  }
}
