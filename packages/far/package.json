{
  "name": "@endo/far",
  "version": "0.2.13",
  "description": "Helpers for distributed objects.",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "build": "exit 0",
    "lint-fix": "yarn lint:eslint --fix && yarn lint:types",
    "lint-check": "yarn lint",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc -p jsconfig.json",
    "lint:eslint": "eslint '**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git"
  },
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "homepage": "https://github.com/endojs/endo#readme",
  "dependencies": {
    "@endo/eventual-send": "^0.16.7",
    "@endo/marshal": "^0.8.0"
  },
  "devDependencies": {
    "@endo/init": "^0.5.51",
    "@endo/ses-ava": "^0.2.35",
    "ava": "^5.0.1",
    "c8": "^7.7.3"
  },
  "keywords": [
    "eventual send",
    "wavy dot",
    "remote objects",
    "tildot",
    "far"
  ],
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  }
}
