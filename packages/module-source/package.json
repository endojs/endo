{
  "name": "@endo/module-source",
  "version": "1.3.3",
  "description": "Ponyfill for the SES ModuleSource and module-to-program transformer",
  "keywords": [
    "ses",
    "ModuleSource",
    "modules",
    "ESM"
  ],
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/module-source#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/module-source"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "exports": {
    ".": {
      "xs": "./src-xs/index.js",
      "default": "./index.js"
    },
    "./shim.js": "./shim.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -fX \"*.d.ts*\" \"*.d.cts*\" \"*.d.mts*\" \"*.tsbuildinfo\"",
    "cover": "c8 ava --config test/_ava-ses.config.js",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc",
    "lint:eslint": "eslint .",
    "lint-fix": "eslint --fix .",
    "test": "yarn test:ses && yarn test:noses",
    "test:ses": "ava --config test/_ava-ses.config.js",
    "test:noses": "ava --config test/_ava-noses.config.js",
    "test:xs": "node scripts/generate-test-xs.js && xst tmp/test-xs.js"
  },
  "dependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "~7.28.3",
    "@babel/traverse": "~7.28.3",
    "@babel/types": "~7.28.2",
    "@endo/harden": "workspace:^",
    "ses": "workspace:^"
  },
  "devDependencies": {
    "ava": "catalog:dev",
    "babel-eslint": "^10.1.0",
    "benchmark": "^2.1.4",
    "c8": "catalog:dev",
    "eslint": "catalog:dev",
    "typescript": "~5.9.2"
  },
  "files": [
    "./*.d.ts",
    "./*.js",
    "./*.map",
    "LICENSE*",
    "SECURITY*",
    "dist",
    "lib",
    "src",
    "tools"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ],
    "ignorePatterns": [
      "test/fixtures/"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.*"
    ],
    "timeout": "2m"
  },
  "typeCoverage": {
    "atLeast": 40.33
  }
}
