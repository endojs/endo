{
  "name": "@endo/fae",
  "version": "0.1.0",
  "private": true,
  "description": "LLM agent loop for Endo with JS evaluation in daemon workers",
  "keywords": [],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/fae"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "exports": {
    ".": "./agent.js",
    "./src/fae-tool-interface.js": "./src/fae-tool-interface.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "setup": "endo run --UNCONFINED setup.js --powers AGENT -E LAL_HOST=$LAL_HOST -E LAL_MODEL=$LAL_MODEL -E LAL_AUTH_TOKEN=$LAL_AUTH_TOKEN",
    "setup-tools": "endo run --UNCONFINED setup-tools.js --powers AGENT",
    "setup-with-tools": "endo run --UNCONFINED setup-with-tools.js --powers AGENT -E LAL_HOST=$LAL_HOST -E LAL_MODEL=$LAL_MODEL -E LAL_AUTH_TOKEN=$LAL_AUTH_TOKEN",
    "setup-fs-tools": "endo run --UNCONFINED setup-fs-tools.js --powers AGENT",
    "lint": "yarn lint:eslint",
    "lint-fix": "yarn lint:eslint --fix",
    "lint:eslint": "eslint '**/*.js'"
  },
  "dependencies": {
    "@endo/daemon": "workspace:^",
    "@endo/errors": "workspace:^",
    "@endo/eventual-send": "workspace:^",
    "@endo/exo": "workspace:^",
    "@endo/lal": "workspace:^",
    "@endo/marshal": "workspace:^",
    "@endo/patterns": "workspace:^",
    "@endo/promise-kit": "workspace:^"
  },
  "devDependencies": {
    "@endo/cli": "workspace:^",
    "eslint": "catalog:dev"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  }
}
