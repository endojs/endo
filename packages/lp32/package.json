{
  "name": "@endo/lp32",
  "version": "1.1.13",
  "description": "32 bit unsigned host byte order length prefix message streams as async iterators",
  "keywords": [
    "stream",
    "endo",
    "chrome",
    "extension",
    "native",
    "host",
    "messaging",
    "length-prefix",
    "async",
    "iterator"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/blob/master/packages/lp32/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/lp32"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./types.d.ts",
  "exports": {
    ".": {
      "types": "./types.d.ts",
      "default": "./index.js"
    },
    "./reader.js": "./reader.js",
    "./writer.js": "./writer.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -fX \"*.d.ts*\" \"*.d.cts*\" \"*.d.mts*\" \"*.tsbuildinfo\"",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint-fix": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:types": "tsc",
    "test": "ses-ava",
    "test:c8": "c8 ${C8_OPTIONS:-} ses-ava"
  },
  "dependencies": {
    "@endo/errors": "workspace:^",
    "@endo/init": "workspace:^",
    "@endo/stream": "workspace:^",
    "ses": "workspace:^"
  },
  "devDependencies": {
    "@endo/ses-ava": "workspace:^",
    "ava": "catalog:dev",
    "c8": "catalog:dev",
    "eslint": "catalog:dev",
    "typescript": "~5.9.2"
  },
  "files": [
    "./*.d.ts",
    "./*.js",
    "./*.map",
    "LICENSE*",
    "SECURITY*",
    "dist",
    "lib",
    "src",
    "tools"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "sesAvaConfigs": {
    "lockdown": "../../ava-endo-lockdown.config.mjs",
    "unsafe": "../../ava-endo-lockdown-unsafe.config.mjs"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
