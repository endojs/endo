{
  "name": "@endo/lp32",
  "version": "0.3.22",
  "description": "32 bit unsigned host byte order length prefix message streams as async iterators",
  "keywords": [
    "stream",
    "endo",
    "chrome",
    "extension",
    "native",
    "host",
    "messaging",
    "length-prefix",
    "async",
    "iterator"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/netstring#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./reader.js": "./reader.js",
    "./writer.js": "./writer.js",
    "./package.json": "./package.json"
  },
  "scripts": {
