{
  "name": "@endo/lp32",
  "version": "1.1.5",
  "description": "32 bit unsigned host byte order length prefix message streams as async iterators",
  "keywords": [
    "stream",
    "endo",
    "chrome",
    "extension",
    "native",
    "host",
    "messaging",
    "length-prefix",
    "async",
    "iterator"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/blob/master/packages/lp32/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/lp32"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./types.d.ts",
  "exports": {
    ".": {
      "types": "./types.d.ts",
      "default": "./index.js"
    },
    "./reader.js": "./reader.js",
    "./writer.js": "./writer.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -f '*.d.ts*'",
    "cover": "c8 ava",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint-fix": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:types": "tsc",
    "test": "ava"
  },
  "dependencies": {
    "@endo/errors": "workspace:^",
    "@endo/init": "workspace:^",
    "@endo/stream": "workspace:^",
    "ses": "workspace:^"
  },
  "devDependencies": {
    "@endo/ses-ava": "workspace:^",
    "ava": "^6.1.3",
    "babel-eslint": "^10.1.0",
    "c8": "^7.14.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "typescript": "~5.6.1-rc"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "ava": {
    "files": [
      "test/**/test-*.*",
      "test/**/*.test.*"
    ],
    "timeout": "2m"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
