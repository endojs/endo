  "devDependencies": {
    "@endo/eslint-config": "^0.5.1",
    "@endo/ses-ava": "^0.2.36",
    "ava": "^5.1.0",
    "babel-eslint": "^10.0.3",
    "c8": "^7.7.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.8.0",
    "typescript": "~4.8.4"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
