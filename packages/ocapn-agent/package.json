{
  "name": "@endo/ocapn-agent",
  "version": "1.1.13",
  "private": true,
  "description": "LLM-powered agent system that generates and executes JavaScript code in isolated SES compartments",
  "keywords": [
    "agent",
    "llm",
    "ses",
    "compartment",
    "ocapn"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/ocapn-agent#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/ocapn-agent"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "bin": "./bin/ocapn-agent.js",
  "dependencies": {
    "@endo/init": "workspace:^",
    "dotenv": "^16.4.5",
    "openai": "^4.77.0"
  },
  "scripts": {
    "build": "exit 0",
    "lint": "yarn lint:types && yarn lint:eslint",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -fX \"*.d.ts*\" \"*.d.cts*\" \"*.d.mts*\" \"*.tsbuildinfo\"",
    "lint-fix": "yarn lint:eslint --fix && yarn lint:types",
    "lint:eslint": "eslint '**/*.js'",
    "lint:types": "tsc",
    "test": "ava",
    "test:c8": "c8 ${C8_OPTIONS:-} ses-ava",
    "test:xs": "exit 0"
  },
  "devDependencies": {
    "@endo/lockdown": "workspace:^",
    "@endo/ses-ava": "workspace:^",
    "ava": "catalog:dev",
    "c8": "catalog:dev",
    "eslint": "catalog:dev",
    "tsd": "catalog:dev",
    "typescript": "~5.9.2"
  },
  "files": [
    "./*.d.ts",
    "./*.js",
    "./*.map",
    "LICENSE*",
    "SECURITY*",
    "dist",
    "lib",
    "src",
    "tools"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.*"
    ],
    "timeout": "2m"
  }
}
