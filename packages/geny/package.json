{
  "name": "@endo/geny",
  "version": "0.0.0",
  "private": true,
  "description": "OCapN host for spawning and managing child processes",
  "keywords": [
    "ocapn",
    "process",
    "spawn",
    "capability"
  ],
  "author": "Endo contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/geny#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/geny"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "exports": {
    ".": "./index.js",
    "./child": "./src/child.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "geny-child": "./src/child.js"
  },
  "scripts": {
    "build": "exit 0",
    "lint": "yarn lint:types && yarn lint:eslint",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -fX \"*.d.ts*\" \"*.d.cts*\" \"*.d.mts*\" \"*.tsbuildinfo\"",
    "lint-fix": "yarn lint:eslint --fix && yarn lint:types",
    "lint:eslint": "eslint '**/*.js'",
    "lint:types": "tsc",
    "test": "ava",
    "test:c8": "c8 ${C8_OPTIONS:-} ses-ava",
    "test:xs": "exit 0"
  },
  "dependencies": {
    "@endo/errors": "workspace:^",
    "@endo/eventual-send": "workspace:^",
    "@endo/far": "workspace:^",
    "@endo/init": "workspace:^",
    "@endo/marshal": "workspace:^",
    "@endo/ocapn": "workspace:^",
    "@endo/pass-style": "workspace:^",
    "@endo/promise-kit": "workspace:^"
  },
  "devDependencies": {
    "@endo/lockdown": "workspace:^",
    "@endo/ses-ava": "workspace:^",
    "ava": "catalog:dev",
    "c8": "catalog:dev",
    "eslint": "catalog:dev",
    "tsd": "catalog:dev",
    "typescript": "~5.9.2"
  },
  "files": [
    "./*.d.ts",
    "./*.js",
    "./*.map",
    "LICENSE*",
    "SECURITY*",
    "dist",
    "lib",
    "src",
    "tools"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.*"
    ],
    "timeout": "2m"
  }
}
