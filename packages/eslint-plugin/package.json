{
  "name": "@endo/eslint-plugin",
  "version": "2.2.1",
  "description": "ESLint plugin for using Endo",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Endo contributors",
  "main": "./lib/index.js",
  "scripts": {
    "test": "mocha",
    "test:xs": "exit 0",
    "build": "exit 0",
    "lint-fix": "exit 0",
    "lint-check": "exit 0",
    "postpack": "git clean -f '*.d.ts*' '*.tsbuildinfo'"
  },
  "dependencies": {
    "requireindex": "~1.2.0",
    "tsutils": "~3.21.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^7.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^10",
    "eslint": "^8.57.0",
    "mocha": "^10.6.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "repository": {
    "directory": "packages/eslint-plugin"
  },
  "files": [
    "./*.d.ts",
    "./*.js",
    "./*.map",
    "LICENSE*",
    "SECURITY*",
    "dist",
    "lib",
    "src",
    "tools"
  ]
}
