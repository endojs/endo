{
  "name": "@endo/ses-ava",
  "version": "1.4.0",
  "description": "Virtualize AVA's test to work better under SES.",
  "keywords": [
    "ses",
    "ava"
  ],
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/endo/tree/master/packages/ses-ava#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/ses-ava"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "type": "module",
  "main": "./index.js",
  "bin": "bin/ses-ava",
  "exports": {
    ".": "./index.js",
    "./test.js": {
      "ses-ava:endo": "./prepare-endo.js",
      "default": "./src/reexport-ava.js"
    },
    "./exported.js": "./exported.js",
    "./prepare-endo.js": "./prepare-endo.js",
    "./prepare-endo-config.js": "./prepare-endo-config.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -fX \"*.d.ts*\" \"*.d.cts*\" \"*.d.mts*\" \"*.tsbuildinfo\"",
    "cover": "c8 ava --config test/_ava-ses.config.js",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint-fix": "eslint --fix .",
    "lint:eslint": "eslint .",
    "lint:types": "tsc",
    "test": "ava && bin/ses-ava --only env --only raw"
  },
  "dependencies": {
    "@endo/env-options": "workspace:^",
    "@endo/harden": "workspace:^",
    "@endo/init": "workspace:^",
    "ava": "^5.3.0 || ^6.1.2",
    "ses": "workspace:^"
  },
  "devDependencies": {
    "@endo/panic": "workspace:^",
    "c8": "catalog:dev",
    "eslint": "catalog:dev",
    "typescript": "~5.9.2"
  },
  "files": [
    "./*.d.ts",
    "./*.js",
    "./*.map",
    "LICENSE*",
    "SECURITY*",
    "bin",
    "dist",
    "lib",
    "src",
    "tools"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.*"
    ],
    "timeout": "2m"
  },
  "sesAvaConfigs": {
    "env": "test/_ava-environment.config.js",
    "not": "test/_ava-must-not-test.config.js",
    "raw": "test/_ava-ses-ava-is-ava.config.js"
  },
  "typeCoverage": {
    "atLeast": 72.22
  }
}
