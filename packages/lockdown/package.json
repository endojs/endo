{
  "name": "@endo/lockdown",
  "version": "1.0.9",
  "description": "Wrappers for hardening JavaScript for Endo",
  "type": "module",
  "main": "pre.js",
  "exports": {
    ".": "./pre.js",
    "./pre.js": "./pre.js",
    "./post.js": "./post.js",
    "./commit.js": "./commit.js",
    "./commit-debug.js": "./commit-debug.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "exit 0",
    "test": "exit 0",
    "test:xs": "exit 0",
    "lint-check": "yarn lint",
    "lint-fix": "eslint --fix '**/*.js'",
    "lint": "eslint '**/*.js'"
  },
  "dependencies": {
    "ses": "^1.7.0"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/lockdown"
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "homepage": "https://github.com/endojs/endo#readme",
  "ava": {
    "files": [
      "test/**/test-*.*",
      "test/**/*.test.*"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:@endo/internal"
    ]
  },
  "eslintIgnore": [
    "bundle-*.js"
  ],
  "publishConfig": {
    "access": "public"
  }
}
