{
  "name": "@endo/lockdown",
  "version": "0.1.25",
  "description": "Wrappers for hardening JavaScript for Endo",
  "type": "module",
  "main": "pre.js",
  "scripts": {
    "build": "exit 0",
    "test": "exit 0",
    "test:xs": "exit 0",
    "lint-check": "yarn lint",
    "lint-fix": "eslint --fix '**/*.js'",
    "lint": "eslint '**/*.js'"
  },
  "devDependencies": {
    "ava": "^5.1.0"
  },
  "dependencies": {
    "ses": "^0.18.1"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git"
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "homepage": "https://github.com/endojs/endo#readme",
  "ava": {
    "files": [
      "test/**/test-*.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
  },
  "eslintIgnore": [
    "bundle-*.js"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "trailingComma": "all",
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  }
}
