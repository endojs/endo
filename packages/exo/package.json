    "lint:eslint": "eslint .",
    "lint:types": "tsc",
    "test": "ava"
  },
  "dependencies": {
    "@endo/common": "workspace:*",
    "@endo/env-options": "workspace:*",
    "@endo/errors": "workspace:*",
