    "@endo/far": "workspace:*",
    "@endo/pass-style": "workspace:*",
    "@endo/patterns": "workspace:*"
  },
  "devDependencies": {
    "@endo/init": "workspace:*",
    "@endo/ses-ava": "workspace:*",
    "ava": "^6.1.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
